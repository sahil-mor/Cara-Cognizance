<div class="text-centre col-12 ">
  <div class="text-center">
    <% if(error && error.length > 0){ %>
    <div class="alert alert-danger" >
      <%= error %> 
    </div>
    <% } %>
  
    <% if(success && success.length > 0){ %>
    <div class="alert alert-success">
      <%= success %>
    </div>
    <% } %>
  </div>
</div>